package java_test;

import java.util.Scanner;

public class test4 {

	public static void main(String[] args) {

		
		
		Scanner in = new Scanner(System.in);
		
		System.out.println("원화를 입력하시오 : ");
		int won = in.nextInt();
		
		double dal = won / 1100;
		
		System.out.printf("%d은 $%.2f",won,dal);
		
	}

}
