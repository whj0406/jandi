package javabean;

public class ScoreBean {

		private String name;		//이름
		private int point;			//성적
		
		//메뉴 [source]/Generate Getter and Setter  ...] 를 이용 자동 생성
		public String getName() {
			return name;
		}
		public void setName(String name) {
			this.name = name;
		}
		public int getPoint()	{
			return point;
		}
		public void setPoint(int point)	{
			this.point = point;
		}
		
		
		

}
