public class Multiplication {

  public static void main(String[] args) {
    int i = 1;
    while (i++ < 9) {
      int j = 0;
      while (j++ < 9) {
        System.out.print(i + " * " + j + " = " + String.format("%2d", i * j));
        System.out.print("    ");
      }
      System.out.println();
    }
  }
}
